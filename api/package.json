{
  "name": "node-rem",
  "version": "0.4.5",
  "description": "Node REM - NodeJS Rest Express MongoDB and more: typescript, passport, JWT, socket.io, HTTPS, HTTP2, async/await, etc.",
  "author": "Duc Nguyen",
  "main": "./src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "yarn": "*"
  },
  "scripts": {
    "start2": "ts-node --downlevelIteration src/index.ts",
    "start": "tsc --downlevelIteration && node dist/index.js",
    "logs2": "forever logs",
    "build-up": "nodemon src/index.ts",
    "build": "tsc --downlevelIteration",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node --inspect src/index.ts",
    "dev": "nodemon",
    "lint": "tslint --project tsconfig.json src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcov mocha --compilers ts:ts-node/register --full-trace --bail --timeout 20000 tests/**/*.ts",
    "test:unit": "cross-env NODE_ENV=test mocha tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "codacy": "cat coverage/lcov.info | codacy-coverage -t $CODACY_PROJECT_TOKEN",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "validate": "yarn lint && yarn test",
    "postpublish": "git push --tags",
    "deploy": "sh ./deploy.sh",
    "docs": "apidoc -i src -o docs",
    "postdocs": "opn docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngduc/node-rem.git"
  },
  "keywords": [
    "rem",
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "mocha",
    "istanbul",
    "nyc",
    "eslint",
    "Travis CI",
    "coveralls",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project",
    "typescript",
    "tslint"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@sendgrid/helpers": "^7.4.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/express-serve-static-core": "^4.17.19",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.3",
    "cross-env": "^5.0.1",
    "dotenv-safe": "^6.0.0",
    "exceljs": "^4.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validation": "^1.0.2",
    "fs": "0.0.1-security",
    "helmet": "^3.13.0",
    "http-status": "^1.0.1",
    "joi": "^10.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "mjml": "^4.3.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "mongoose": "^6.0.5",
    "morgan": "^1.9.1",
    "mstime": "^0.6.1",
    "multer": "^1.4.1",
    "passport": "^0.4.1",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.0",
    "passport-jwt-cookiecombo": "^1.0.5",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "querystring": "^0.2.0",
    "request-promise": "^4.2.2",
    "spdy": "^3.4.7",
    "tempfile": "^2.0.0",
    "uid-safe": "^2.1.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/cheerio": "^0.22.10",
    "@types/exceljs": "^1.3.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/joi": "^13.6.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.118",
    "@types/mjml": "^4.0.1",
    "@types/mocha": "^5.2.5",
    "@types/moment-timezone": "^0.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.3.7",
    "@types/node": "^8.10.66",
    "@types/passport": "^1.0.6",
    "@types/pug": "^2.0.4",
    "@types/request-promise": "^4.1.42",
    "@types/sendgrid": "^4.3.0",
    "@types/tempfile": "^2.0.0",
    "apidoc": "^0.17.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codacy-coverage": "^3.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "forever": "^0.15.3",
    "husky": "^1.1.1",
    "mocha": "^3.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "opn-cli": "^3.1.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsc": "^1.20150623.0",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^4.0.5"
  }
}
